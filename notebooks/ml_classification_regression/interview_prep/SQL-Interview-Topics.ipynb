    "        \n",
    "        LAG(Champion, 1) OVER (ORDER BY Year ASC) AS Last_Champion\n",
    "        LAG(Champion, 1) OVER (PARTITION BY Event ORDER BY Event ASC, Year ASC) AS Last_Champion\n",
